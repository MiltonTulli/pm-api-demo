{
  "name": "pm-api",
  "version": "1.11.0",
  "description": "pm REST API",
  "private": true,
  "main": "server.js",
  "scripts": {
    "start:docker": "docker-compose up",
    "dev": "nodemon --watch src server.js",
    "lint": "eslint --format compact .",
    "test": "NODE_ENV=test GROWSURF_API=https://api.growsurf.com/v2 GROWSURF_CAMPAIGN_ID=XXXXXX GROWSURF_API_KEY=XXXXXXX nyc mocha 'test/**/*.spec.js'",
    "test:docker": "docker-compose run --rm api npm test",
    "start:worker": "node src/jobs/extractPatientEHRs/server.js",
    "dev:worker": "nodemon --watch src src/jobs/extractPatientEHRs/server.js",
    "migrate:create": "node ./node_modules/migrate-mongo/bin/migrate-mongo.js create",
    "migrate:status": "node ./node_modules/migrate-mongo/bin/migrate-mongo.js status",
    "migrate:up": "node ./node_modules/migrate-mongo/bin/migrate-mongo.js up",
    "migrate:down": "node ./node_modules/migrate-mongo/bin/migrate-mongo.js down",
    "migrate:status:docker": "docker-compose run --rm api node ./node_modules/migrate-mongo/bin/migrate-mongo.js status",
    "migrate:up:docker": "docker-compose run --rm api node ./node_modules/migrate-mongo/bin/migrate-mongo.js up",
    "migrate:down:docker": "docker-compose run --rm api node ./node_modules/migrate-mongo/bin/migrate-mongo.js down",
    "deploy": "sls deploy"
  },
  "engines": {
    "node": "8.10.0",
    "npm": "5.6.0"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "chai-http": "^4.2.1",
    "chance": "^1.0.18",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.17.2",
    "migrate-mongo": "^5.0.1",
    "mocha": "^6.1.4",
    "mockdate": "^2.0.2",
    "nock": "^10.0.6",
    "nodemon": "^1.18.11",
    "nyc": "^14.0.0",
    "pem-jwk": "^2.0.0",
    "serverless": "^1.41.0",
    "sinon": "^7.3.2"
  },
  "dependencies": {
    "aws-cognito-express": "^1.3.0",
    "aws-sdk": "^2.440.0",
    "axios": "^0.18.0",
    "bluebird": "^3.5.4",
    "body-parser": "^1.18.3",
    "celebrate": "^9.1.0",
    "cognito-express": "^2.0.14",
    "cors": "^2.8.5",
    "debug": "^4.1.1",
    "express": "^4.16.4",
    "helmet": "^3.16.0",
    "http-errors": "^1.7.2",
    "http-status": "^1.3.2",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.11",
    "moment": "^2.24.0",
    "mongoose": "^5.5.2",
    "mongoose-paginate": "^5.0.3",
    "mongoose-paginate-v2": "^1.3.0",
    "morgan": "^1.9.1",
    "require-all": "^3.0.0",
    "serverless-http": "^2.0.0",
    "validator": "^10.11.0",
    "winston": "^3.2.1"
  }
}
